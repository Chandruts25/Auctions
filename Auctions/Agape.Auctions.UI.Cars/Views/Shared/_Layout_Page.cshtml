    <script>
     
        $(window).load(function () {
            var url = window.location.href;
            $('ul.rd-navbar-nav li.rd-nav-item').find('.active').removeClass('active');
            $('ul.rd-navbar-nav li.rd-nav-item a').filter(function () {
                return this.href == url;
            }).parent().addClass('active');


            var str = "https://localhost:44389/Auctions/Details?carId=104877c0-b069-47d9-be05-7529d811ca62";

            var rest = url.substring(0, url.lastIndexOf("/") + 0);
            var ff = rest.substring(rest.lastIndexOf("/") + 1, str.length);

            console.log(ff);


           



            if (ff == "Auctions") {

                $(".auction_class").parent().addClass('active');
            }




        });
    </script>

